	<%= form_for(rate, remote: true) do |f| %>
	  <button class="up-vote" type="submit" onclick="plus_one('rating-<%= rating.id %>');">
	    <i class="fa fa-arrow-circle-up"> </i>
	  </button>
	<% end %>

	<%= form_for(rate, html: { method: :delete }, remote: true) do |f| %>
	<%= hidden_field_tag :rating_id, rating.id %>
     <button class="un-down-vote" type="submit" onclick="do_nothing('rating-<%= rating.id %>');">
       <i class="fa fa-arrow-circle-down"> </i>
     </button>
	<% end %>
